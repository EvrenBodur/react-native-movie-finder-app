{
  "expo": {
    "name": "movie-finder-react-native",
    "slug": "movie-finder-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "movie finder app using redux-thunk",
    "githubUrl": "https://github.com/EvrenBodur/react-native-movie-finder-app.git",
    "android": {
      "package": "com.evrenbodur.moviefinderreactnative"
    }
  }
}
